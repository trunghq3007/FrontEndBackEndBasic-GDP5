		<script>

			function scrollToTop() {
				$('html, body').animate({ scrollTop: 0 }, 800);
			}

			function scrollToRegiste() {
				$('html, body').animate({ scrollTop: 200 }, 800);
			}

			/* Start Click đăng ký handler */
			var hasBeenClicked = false;
			$(document).ready(function () {
				 $("#iPhone").keyup(function () {
				 	if (this.value.match(/[^0-9.]/g)) {
				 		this.value = this.value.replace(/[^0-9]/g, '');
				 	}
				 });
				 var iName = $("#iName");
				 var iPhone = $("#iPhone");
				 var iNoio = $("#iNoio");
				 var iTotNghiep = $("#iTotNghiep");
				 var iNganh = $("#iNganh");
				 var iEmail = $("#iEmail");
				$('#registe_btn').click(function () {
					 if (!hasBeenClicked) {
					 	hasBeenClicked = true;
					 	if (iName.val() != "" && iPhone.val() != "" && iEmail.val() != "" && validateEmail(iEmail.val()) == true && iPhone.val().length > 8 && iTotNghiep.val() != 0 && iNganh.val() != 0 && iNoio.val() != 0) {
					 		$("#loading_wrap").toggle();
					 		var txtName = iName.val();
					 		var txtMobile = iPhone.val();
					 		var txtEmail = iEmail.val();
					 		var txtNganh = iNganh.val();
					 		var txtNoio = iNoio.val();
					 		var txtTotNghiep = iTotNghiep.val();
					 		var url = location.href;
					 		$("#form_id").attr("action", "ContactServlet");
					 	}
						 else if (iName.val() == "") {
						 	$("#note").html("Bạn chưa nhập tên");
						 	hasBeenClicked = false;
						 }
						 else if (iPhone.val().length < 9) {
						 	$("#note").html("Bạn chưa nhập đủ số điện thoại");
						 	hasBeenClicked = false;
						 }
						 else if (iNoio.val() == 0 || iNoio.val() == "" || iNoio.val() == "Nơi ở...") {
						 	$("#note").html("Bạn cần chọn nơi ở");
						 	hasBeenClicked = false;
						 }
						 else if (iTotNghiep.val() == 0 || iTotNghiep.val() == "" || iTotNghiep.val() == "Bạn đã tốt nghiệp ?") {
						 	$("#note").html("Bạn cần chọn cấp học đã tốt nghiệp");
						 	hasBeenClicked = false;
						 }
						 else if (iNganh.val() == 0 || iNganh.val() == "" || iNganh.val() == "Ngành Đăng Ký") {
						 	$("#note").html("Bạn cần chọn ngành học");
						 	hasBeenClicked = false;
						 }
						 else if (validateEmail(iEmail.val()) == false) {
						 	$("#note").html("Bạn chưa nhập hoặc nhập sai email");
						 	hasBeenClicked = false;
						 }
					 }
					 });
				});
			function validateEmail(email) {
				var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
				if (reg.test(email) == false) {
					return false;
				}
				else {
					return true;
				}
			}
			function sleep(milliseconds) {
				var start = new Date().getTime();
				for (var i = 0; i < 1e7; i++) {
					if ((new Date().getTime() - start) > milliseconds) {
						break;
					}
				}
			}
/* End Click đăng ký handler */
